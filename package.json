{
  "name": "my-react-router-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "build:mrt": "vite build --config vite.mrt.config.ts",
    "build:prod": "vite build --config vite.prod.config.ts",
    "build:all": "npm run build && npm run build:mrt && npm run build:prod",
    "dev": "cross-env NODE_ENV=development vite",
    "start": "cross-env NODE_ENV=production node dist/prod/server.js",
    "typecheck": "tsc --noEmit",
    "bridge": "node bridge.cjs",
    "push": "node upload.cjs",
    "tail-logs": "npx @salesforce/pwa-kit-dev@latest tail-logs --project scaffold-pwa --environment kev"
  },
  "dependencies": {
    "@remix-run/node-fetch-server": "0.8.0",
    "aws-sdk": "^2.1692.0",
    "aws-serverless-express": "^3.4.0",
    "compression": "^1.8.0",
    "cross-env": "^7.0.3",
    "express": "^5.1.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-router": "7.7.0"
  },
  "devDependencies": {
    "@tailwindcss/typography": "0.5.16",
    "@tailwindcss/vite": "^4.1.10",
    "@types/compression": "^1.8.1",
    "@types/express": "^5.0.3",
    "@types/node": "^24.0.3",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.5.2",
    "@vitejs/plugin-rsc": "^0.4.11",
    "archiver": "^1.3.0",
    "chalk": "^4.1.2",
    "fs-extra": "^11.1.1",
    "minimatch": "^3.1.2",
    "node-fetch": "^2.6.9",
    "tailwindcss": "^4.1.10",
    "typescript": "^5.8.3",
    "validator": "^13.9.0",
    "vite": "^6.3.5",
    "vite-plugin-devtools-json": "0.2.0"
  }
}
